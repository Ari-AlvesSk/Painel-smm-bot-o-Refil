      <!-- Stat main -->
      <main data-spy="scroll" data-target="#navbar-example2" data-offset="0">
        <!-- Start Banner Section -->
        <section class="demo_2 demo_4 banner_section" id="Vision">
          <div class="container">
            <div class="row justify-content-md-center text-center">
              <div class="col-md-10 col-lg-6">
                <div class="banner_title">
                  <div class="offer">
                    <span class="c-magenta">{{ site['site_name'] }}</span>
                  </div>
                  <h1 class="c-white">Türkiye'nin En Gelişmiş SMM Paneli</h1>
                  <p>
                    7/24 Tam otomatik alt yapısı sayesinde bütün işlemlerinizi hızlı ve kolayca halledebileceğiniz panelimize hoşgeldiniz.
                  </p>
                </div>
                <div class="button--click">
                    {% if errorText %}
        <div class="alert alert-dismissible alert-danger">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ errorText }}
        </div>
      {% endif %}
      {% if successText %}
        <div class="alert alert-dismissible alert-success">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ successText }}
        </div>
      {% endif %}

                  <button type="button"
                    class="btn btn-token wd-140 btn_md_primary scale effect-letter c-gradient mb-3 mb-sm-0" data-toggle="modal" data-target="#mdllLogin">
                    {{ lang['signin.title'] }}
                  </button>
                  <a href="/signup" class="btn btn-token wd-140 btn_md_primary effect-letter mb-3 mb-sm-0 defalult">
                    {{ lang['signup.title'] }}
                  </a>
                </div>
              </div>
              <div class="img__head">
                <img class="illy" src="assets/img/crypto/crypto-head02.png" />
              </div>
              
            <div class="about_currency padding-t-12">
              <div class="row justify-content-md-center">
                <div class="col-md-10 col-lg-6 text-center">
                  <div class="title_sections mb-0">
                    <h2>Vizyonumuz</h2>
                    <p>
                      Bütün müşterilerimizi sorunsuz ve hızlı servisler ile buluşturup ucuz fiyat politikamız ile düzenli ve kaliteli hizmet vermek.
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- End Banner -->

        <!-- Start form Contact -->
       <section class="contact_section padding-t-2" id="Contact">
          <img class="img-fluid" src="assets/img/crypto/02.png" />
          <div class="container">
            <div class="row justify-content-md-center">
              <div class="col-md-8 col-lg-3 text-center">
                <div class="title_sections">
                  <h2 class="c-white">Fenomen Olmaya Hazır Mısın?</h2>
                  <p class="c-currency-p">
                    O Halde Ne Duruyorsun?
Hemen Kayıt Ol ve Fenomenlik Yolunda İlk Adımı At.
                  </p>
                </div>
              </div>
            </div>
            <div class="form-row justify-content-md-center margin-t-2">
              <form method="post" action="signup">
         <div class="form-group dark-form">
            <label for="username" class="control-label" style="color:#fff;">{{ lang['signup.username'] }}</label>
            <input type="text" class="form-control" name="username" value="{{ data['username'] }}">
         </div>  
        {% if settings['name_secret'] == 2 %}
          <div class="form-group dark-form">
            <label for="username" class="control-label" style="color:#fff;">{{ lang['signup.first_name'] }}</label>
            <input type="text" class="form-control"  name="first_name" value="{{ data['first_name'] }}">
          </div>
          <div class="form-group dark-form">
            <label for="username" class="control-label" style="color:#fff;">{{ lang['signup.last_name'] }}</label>
            <input type="text" class="form-control"  name="last_name" value="{{ data['last_name'] }}">
          </div>
		{% endif %}
          <div class="form-group dark-form">
            <label for="username" class="control-label" style="color:#fff;">{{ lang['signup.email'] }}</label>
            <input type="text" class="form-control" name="email" value="{{ data['email'] }}">
          </div>
        {% if settings['skype_area'] == 2 %}
          <div class="form-group dark-form">
            <label for="username" class="control-label" style="color:#fff;">{{ lang['signup.telephone'] }}</label>
            <input type="text" class="form-control" name="telephone" value="{{ data['telephone'] }}">
          </div>
		{% endif %}
          <div class="form-group dark-form">
            <label for="password" class="control-label" style="color:#fff;">{{ lang['signup.password'] }}</label>
            <input type="password" class="form-control" name="password">
          </div>
          <div class="form-group dark-form">
            <label for="password" class="control-label" style="color:#fff;">{{ lang['signup.confirm'] }}</label>
            <input type="password" class="form-control" name="password_again">
          </div>
        {% if settings['terms_checkbox'] == 2 %}
          <div class="checkbox">
            <label>
              <input type="checkbox" name="terms" value="1"><font style="color:#fff;">  {{ lang['signup.accept_terms_text'] }}</font>
            </label>
          </div>
        {% endif %}

          <div class="form-group">
             <div class="g-recaptcha" data-sitekey="{{ captchaKey }}"></div>
          </div>

            <button type="submit" class="btn btn-primary btn-lg">{{ lang['signup.button'] }}</button>
        </form>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- End. form Contact -->

      </main>
      <!-- end main -->
    </div>
